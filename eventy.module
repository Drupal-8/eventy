<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help()
 */
function eventy_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
  	case 'help.page.eventy':
  	  $output = '';
  	  $output .= '<h3>' . t('About') . '</h3>';
  	  $output .= '<p>' . t('Placeholder');
  	  return $output;
  }
}